{
  "name": "virtualized-tree-component",
  "version": "1.1.13",
  "description": "A tiny react tree-component based on react-virtualized",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "size-limit": "size-limit",
    "build": "rm -rf dist && parcel build",
    "test": "jest",
    "ci": "npm install",
    "lint": "eslint src/*",
    "prepublishOnly": "npm run size-limit && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xatabch/tree-component.git"
  },
  "peerDependencies": {
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "lodash": "4.17.21",
    "react-virtualized": "9.22.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@size-limit/preset-small-lib": "8.1.0",
    "@types/jest": "29.2.3",
    "@types/lodash": "4.14.190",
    "@types/react": "^16.14.34",
    "@types/react-virtualized": "^9.21.21",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "babel-jest": "29.2.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "parcel": "^2.7.0",
    "size-limit": "8.1.0",
    "typescript": "4.6.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "author": "ivan.gmorozov@gmail.com",
  "license": "ISC",
  "source": "src/index.tsx",
  "size-limit": [
    {
      "limit": "187 B",
      "path": "src/index.tsx"
    }
  ]
}
